{
  "id": null,
  "title": "Master Infrastructure Overview",
  "tags": ["infrastructure", "overview", "master", "monitoring"],
  "style": "dark",
  "timezone": "Asia/Dhaka",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "System Status Overview",
      "type": "stat",
      "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "value": "1",
              "text": "UP"
            },
            {
              "type": "value",
              "value": "0",
              "text": "DOWN"
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "targets": [
        {
          "expr": "up{job=\"node-exporter\"}",
          "legendFormat": "Node Exporter"
        },
        {
          "expr": "up{job=\"cadvisor\"}",
          "legendFormat": "cAdvisor"
        },
        {
          "expr": "up{job=\"nginx-exporter\"}",
          "legendFormat": "Nginx"
        },
        {
          "expr": "up{job=\"prometheus\"}",
          "legendFormat": "Prometheus"
        }
      ]
    },
    {
      "id": 2,
      "title": "CPU Usage",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 0, "y": 4},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 85
              }
            ]
          },
          "unit": "percent",
          "max": 100,
          "min": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "targets": [
        {
          "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "CPU Usage"
        }
      ]
    },
    {
      "id": 3,
      "title": "Memory Usage",
      "type": "stat",
      "gridPos": {"h": 4, "w": 4, "x": 4, "y": 4},
